depend() {
	need net
}

start() {
	ebegin "Starting sniproxy"
	start-stop-daemon --start --quiet --exec /usr/sbin/sniproxy 
	eend $?
}

stop() {
	ebegin "Stopping sniproxy"
	start-stop-daemon --stop --quiet --pidfile /run/sniproxy.pid
	eend $?
}
